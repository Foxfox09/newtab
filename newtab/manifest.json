{
  "manifest_version": 3,
  "name": "пошукач — Нова вкладка",
  "version": "1.1.2",
  "description": "Персоналізована нова вкладка.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/logof.png",
    "48": "icons/logof.png",
    "128": "icons/logof.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "https://suggestqueries.google.com/"
  ],
  "host_permissions": [
    "https://suggestqueries.google.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self' https://*; script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https://www.google.com/s2/favicons/ https://*; connect-src 'self' https://*;"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "favicon/*",
        "js/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
